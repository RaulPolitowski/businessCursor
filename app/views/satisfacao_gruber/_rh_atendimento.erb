<div class="panel panel-default" style="margin-bottom: 10px !important;">
  <div class="panel-heading">
    Média - RH/Atendimento
  </div>
  <div id="fatDiv" class="col-md-12 padding-5">
    <div id="fatDiv" class="col-lg-12 padding-left-right">
      <div class="ibox">
        <div class="ibox-content product-box">
          <div class="ibox-title ibox-title-without-border-style">
            <h5>Setor RH</h5>
            <span class="label label-info pull-right">Média</span>
          </div>
          <div class="ibox-content padding-5" style="height: 55px !important;">
            <div class="col-lg-6 no-padding">
              <h2 class="pull-center" id="media_nota_rh" style="margin-bottom: 3px"></h2>
            </div>
            <div class="col-lg-6 no-padding">
              <div class="col-lg-12 no-padding pull-center">
                <small>Qtd. Pesquisas</small>
              </div>
              <div class="col-lg-12 no-padding">
                <div class="pull-center">
                  <small id="qtd_pesquisas_rh"></small>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div id="fatDiv" class="col-lg-12 padding-left-right">
      <div class="ibox">
        <div class="ibox-content product-box">
          <div class="ibox-title ibox-title-without-border-style">
            <h5>Setor Atendimento</h5>
            <span class="label label-info pull-right">Média</span>
          </div>
          <div class="ibox-content padding-5" style="height: 55px !important;">
            <div class="col-lg-6 no-padding">
              <h2 class="pull-center" id="media_nota_atendimento" style="margin-bottom: 3px"></h2>
            </div>
            <div class="col-lg-6 no-padding">
              <div class="col-lg-12 no-padding pull-center">
                <small>Qtd. Pesquisas</small>
              </div>
              <div class="col-lg-12 no-padding">
                <div class="pull-center">
                  <small id="qtd_pesquisas_atendimento"></small>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="panel-body no-padding" style="height: 290px !important;">
  </div>
</div>
<script type="text/javascript">
    function drawChartMediaSetoresFinanceiro() {
        $.getJSON("/satisfacao_gruber/media_setores_rh?data_inicial="+ $('#data_inicio').val() + "&data_final="+ $('#data_fim').val(), function(data)
        {
            if(data[0] && data[0].nota){
                $('#media_nota_rh').text(data[0].nota);
                $('#qtd_pesquisas_rh').text(data[0].qtd_pesquisas);
            }else{
                $('#media_nota_rh').text("S/N");
                $('#qtd_pesquisas_rh').text("S/N");
            }
        });
    }
    function drawChartMediaSetoresAtendimento() {
        $.getJSON("/satisfacao_gruber/media_setores_atendimento?data_inicial="+ $('#data_inicio').val() + "&data_final="+ $('#data_fim').val(), function(data)
        {
            if(data[0] && data[0].nota){
                $('#media_nota_atendimento').text(data[0].nota);
                $('#qtd_pesquisas_atendimento').text(data[0].qtd_pesquisas);
            }else{
                $('#media_nota_atendimento').text("S/N");
                $('#qtd_pesquisas_atendimento').text("S/N");
            }
    });
    }


</script>

