<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-lg-10">
    <h2>USUÁRIOS GZAP</h2>
    <ol class="breadcrumb">
      <li>
        <a href="<%= url_for :controller => 'dashboards', :action => 'painel_vendas' %>">Home</a>
      </li> 
      <li class="active">
        <strong>Usuário Gzap</strong>
      </li>
    </ol>
  </div>
</div>

<div class="wrapper wrapper-content animated fadeInRight">
  <div class="row">
    <div class="col-lg-12">
      <div class="ibox float-e-margins">
        <div class="ibox-title">         
          <h5>Usuários Gzap</h5>
          <div class="ibox-tools">
            <%= button_to new_gzap_usuario_path, { method: 'get', class: 'btn btn-sm btn-primary' } do %>
              <i class="fa fa-plus-circle" aria-hidden="true">Adicionar usuário gzap</i>
            <% end %>
          </div>
        </div>
        <div class="ibox-content">
          <div class="tab-content">
            <ul class="nav nav-tabs">
              <li class="active"><a data-toggle="tab" id="tabAtivos" href="#tabAtivos-1">Ativos</a></li>
              <li><a data-toggle="tab" id="tabDesativados" href="#tabDesativados-1">Desativados</a></li>
            </ul>
          </div>
          <div class="tab-content">
            <div id="tabAtivos-1" class="tab-pane active">
              <%= render partial: 'gzap_usuarios/tables/table_gzap_usuario', locals: {
                gzap_usuarios: @gzap_usuarios_ativos
              } %>
            </div>
            <div id="tabDesativados-1" class="tab-pane">
              <%= render partial: 'gzap_usuarios/tables/table_gzap_usuario', locals: {
                gzap_usuarios: @gzap_usuarios_desativados
              } %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>