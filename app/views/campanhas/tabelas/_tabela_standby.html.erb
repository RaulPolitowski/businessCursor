<div class="row">
  <div class="col-lg-12 padding">
    <div class="ibox-content">
      <table class="table table-striped table-bordered table-hover table-campanha-standby" style="font-size: 12px;">
        <thead>
        <tr>
          <th class="tableHeadCampanha" style="width: 10%">Número</th>
          <th>Usuário</th>
          <th class="tableHeadCampanha" style="width: 20%">Status/Maturado</th>
        </tr>
        </thead>
        <tbody id="body_table_campanha_standby">
        <% @standby.each do |number| %>
          <tr>
            <td class="tableHeadCampanha"><%= number.numero %></td>
            <td><%= number.nome %></td>
            <td class="tableHeadCampanha"><%="Aguardando | #{(Time.zone.now.to_date - number.created_at.to_date).to_i} dias"%> </td>
          </tr>
        <% end %>
        </tbody>
        <tfoot>
        </tfoot>
      </table>
    </div>
  </div>
</div>