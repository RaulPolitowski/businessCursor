<div class="row">
  <div class="col-lg-12">
    <div class="ibox float-e-margins">
      <div class="ibox-title">
        <h5>Importações</h5>

        <div class="ibox-tools">
          
          <% if current_user.admin? %>
            <button class="btn btn-sm btn-primary" id="btnReajustarFilas">
              <i class="fa fa-wrench" aria-hidden="true"> Reajustar Filas</i>
            </button>
          <% end %>
          <% if current_user.admin? %>
            <button class="btn btn-sm btn-primary" id="btnReconsultar">
              <i class="fa fa-file" aria-hidden="true"> Reconsultar empresas</i>
            </button>
          <% end %>
          <% if current_user.admin? %>
            <button class="btn btn-sm btn-primary" id="btnReprocessarImportacao">
              <i class="fa fa-file" aria-hidden="true"> Reprocessar importaçao</i>
            </button>
          <% end %>
          <% if current_user.admin? %>
            <button class="btn btn-sm btn-primary" id="btnImportacaoEmMassa">
              <i class="fa fa-file" aria-hidden="true"> Importação em massa</i>
            </button>
          <% end %>
        </div>
      </div>
      <div class="ibox-content">
        <table class="table table-striped table-bordered table-hover table-importacoes">
          <thead>
          <tr>
            <th>Id</th>
            <th>Data importação</th>
            <th>Total</th>
            <th>Importado</th>
            <th>Não importado</th>
            <th>Já existente</th>
            <th>Estado</th>
            <th>Ação</th>
          </tr>
          </thead>
          <tbody style="width:100%" id="tbody_importacoes">

          </tbody>
          <tfoot>
          </tfoot>
        </table>
      </div>
    </div>
  </div>
</div>


  <script src="https://cdn.datatables.net/plug-ins/1.10.19/sorting/date-eu.js"></script>
  <script src="https://cdn.datatables.net/plug-ins/1.10.19/sorting/date-euro.js"></script>
  <script>
      $(document).ready(function () {
          $('.table-importacoes').DataTable({
              pageLength: 10,
              responsive: true,
              dom: '<"html5buttons"B>lTfgitp',
              "order": [
                  [0, "desc"]
              ],
              "ordering": true,
              buttons: [{
                  extend: 'excel',
                  title: 'Importações'
              },
                  {
                      extend: 'pdf',
                      title: 'Importações'
                  }
              ],
              columnDefs: [{
                  type: 'date-eu',
                  targets: 1
              }

              ],
              "language": {
                  "url": "//cdn.datatables.net/plug-ins/1.10.16/i18n/Portuguese-Brasil.json"
              }
          });
      });
  </script>