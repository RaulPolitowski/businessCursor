<div id="modal_usuario_finalizar_implantacao" class="modal inmodal" tabindex="-1" role="dialog" aria-hidden="true" >
  <div class="modal-dialog">
    <div class="modal-content animated fadeIn">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
        <h4 class="modal-title">Informações de conclusão</h4>
      </div>
      <%= form_for @implantacao, :url => finalizar_implantacao_path do |f| %>
          <div class="modal-body scroll" style="min-height: 100px;">
            <% if current_user.admin? %>
              <div class="form-group">
                <div class="field">
                    <label for="responsavel_novo">Implantador</label>
                    <%= f.select :user_id, options_for_select(User.empresas_acesso(current_empresa.id).collect {|p| [ p.name, p.id ] }, current_user.id ), {}, {:class => "form-control input-sm chosen-select"} %>
                </div>
              </div>
            <% end %>
            <div id="perguntas">

            </div>
          </div>
          <div class="modal-footer">
            <button type="button" class="btn btn-white" data-dismiss="modal">Cancelar</button>
            <%= f.submit 'Finalizar', :id => 'btnSubmitFinalizarImplantacao' , :class => 'btn btn-info'%>
          </div>
      <% end %>
    </div>
  </div>
</div>


