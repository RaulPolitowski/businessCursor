    function buscarClientes(status) {
        if($('#status_id').val() == status){
            $('#bloco_clientes').hide();
            $('#status_id').val(null);
        }else{
            $('#status_id').val(status);
            $.getJSON("/dashboards/get_acompanhamentos_table?data_inicio=" + $('#data_inicio').val() +
                "&data_fim=" + $('#data_fim').val() + '&status_id=(' + status +
                ')&vendedor_id=' + $('#vendedor_id').val()
                + '&implantador_id=' + $('#implantador_id').val()
                + '&responsavel_id=' + $('#responsavel_id').val()
                + '&empresa_id=' + $('#empresa_id').val()
                + '&novos=' + $('#novos').val()
                + '&estado=' + $('#estado_financeiro_id').val()
                + '&flag=' + $('#efetivas').val(), function(data) {
                
                var table = $('.table-implantacoes').DataTable();
                table.clear().draw();
                $('#bloco_clientes').show();
                var t = $('.table-implantacoes').DataTable();

                if(jQuery.inArray( status, ['0', '1', '2'] ) != -1){

                    $.each(data,function (i,val){
                        t.row.add( [
                            moment(val['data']).format("DD/MM/YYYY HH:mm"),
                            val['razao_social'],
                            val['dias_sem_uso'],
                            val['cidade'],
                            val['vendedor'],
                            val['implantador'],
                            val['sistema'],
                            mascaraValor(val['valor_mensalidade']),
                            val['honorario_ativo'],
                            (val['data_agendamento_retorno'] != null ? moment(val['data_agendamento_retorno']).format("DD/MM/YYYY HH:mm") : 'Sem retorno'),
                            '<i onclick="abrirModal(' + val['acompanhamentoid'] + ')" class="fa fa-search" aria-hidden="true" title="Ver registro de atividades"></i>',
                            val['motivo']
                        ] ).draw( false );
                    });
                    t.columns( [11] ).visible( true );
                    t.columns.adjust().draw( false );
                }else{
                    $.each(data,function (i,val){
                        t.row.add( [
                            moment(val['data']).format("DD/MM/YYYY HH:mm"),
                            val['razao_social'],
                            val['dias_sem_uso'],
                            val['cidade'],
                            val['vendedor'],
                            val['implantador'],
                            val['sistema'],
                            mascaraValor(val['valor_mensalidade']),
                            val['honorario_ativo'],
                            '',
                            '<i onclick="abrirModal(' + val['acompanhamentoid'] + ')" class="fa fa-search" aria-hidden="true" title="Ver registro de atividades"></i>',
                            val['motivo']
                        ] ).draw( false );
                    });
                    t.columns( [11] ).visible( false );
                    t.columns.adjust().draw( false );
                }

            });
        }
    }

    function createTableAcompanhamentos(){
        if ($("#DataTables_Table_0_wrapper").length == 0){
            $('.table-implantacoes').DataTable({
                pageLength: 10,
                responsive: true,
                dom: '<"html5buttons"B>lTfgitp',
                buttons: [
                    {
                        extend: 'excel',
                        title: 'Acompanhamentos'
                    },
                    {extend: 'pdf', title: 'Acompanhamentos'}

                ],
                "ordering": true,
                "order": [[ 8, "asc" ]],
                "language": {
                    "url": "//cdn.datatables.net/plug-ins/1.10.16/i18n/Portuguese-Brasil.json"
                },
                "columnDefs": [
                    { "width": "15%", "targets": [0,2,3] },
                    { "width": "5%", "targets": [7,8] },
                    { "width": "25%", "targets": 1 },
                    { "width": "15%", "targets": [4,5,6] },
                    { "width": "5%", "targets": [9,10,11] }
                ],
                columnDefs: [
                   { type: 'date-euro', targets: 0 },
                   { type: 'date-euro', targets: 11 },
                  {
                    targets: [11],
                    visible: false
                } ],
                "createdRow": function ( row, data, index ) {
                    if (data[10] != null &&  moment(data[10], "DD/MM/YYYY HH:mm").add(1, 'hours') <= moment()) {
                        $('td', row).addClass('redcolor');
                    }
                }
            });
        }
    }
    function abrirModal(id) {
        $.ajax({
            url: '/acompanhamentos/activities',
            data: {id: id},
            type: 'GET',
            success: function (data) {
                $("#modal_content_activities").html(data);
                $("#modal_activities_acompanhamentos").modal('show');
                addFuncoes();
            },error: function(data){
                exibirErro(data);
            }
        });
