    font-size: 14px;
    color: #676a6c;
  }
  
  .value-increase {
    color: #336699;
  }
  
  .value-decrease {
    color: #993333;
  }
  
  .stat-badge {
    display: inline-block;
    padding: 2px 6px;
    border-radius: 10px;
    font-size: 12px;
    margin-left: 5px;
  }
  
  .badge-increase {
    background-color: rgba(26, 179, 148, 0.15);
    color: #1ab394;
  }
  
  .badge-decrease {
    background-color: rgba(237, 85, 101, 0.15);
